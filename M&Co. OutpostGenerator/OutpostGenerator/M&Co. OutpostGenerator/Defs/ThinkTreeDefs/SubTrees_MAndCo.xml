<?xml version="1.0" encoding="utf-8" ?>
<ThinkTrees>

  <ThinkTreeDef>
    <defName>MAndCoPostDuty</defName>
    <insertTag>Humanlike_PostDuty</insertTag>
    <insertPriority>100</insertPriority>
    <thinkRoot Class="ThinkNode_Priority">
      <subNodes>
        <li Class="OutpostGenerator.ThinkNode_ConditionalMAndCoEmployee">
          <subNodes>
            <!--  Seek allowed area  -->
            <li Class="JobGiver_SeekAllowedArea"/>
            <!--  Seek safe temperatures  -->
            <li Class="JobGiver_SeekSafeTemperature"/>
            <!--  Drop unnused inventory  -->
            <li Class="JobGiver_DropUnusedInventory"/>
            <!--  Get food (only if starving)  -->
            <li Class="ThinkNode_ConditionalStarving">
              <subNodes>
                <li Class="JobGiver_GetFood"/>
              </subNodes>
            </li>
            <!--  Emergency work  -->
            <li Class="JobGiver_Work">
              <emergency>true</emergency>
            </li>
            <!--  Pack food if not hungry -->
            <li Class="ThinkNode_ConditionalNeedPercentageAbove">
              <need>Food</need>
              <threshold>0.6</threshold>
              <subNodes>
                <li Class="JobGiver_PackFood"/>
              </subNodes>
            </li>
          </subNodes>
        </li>
      </subNodes>
    </thinkRoot>
  </ThinkTreeDef>
  
  <ThinkTreeDef>
    <defName>MAndCoPreMain</defName>
    <insertTag>Humanlike_PreMain</insertTag>
    <insertPriority>100</insertPriority>
    <thinkRoot Class="ThinkNode_Priority">
      <subNodes>
        <li Class="OutpostGenerator.ThinkNode_ConditionalMAndCoEmployee">
          <subNodes>
            <li Class="ThinkNode_PrioritySorter">
              <subNodes>
                <li Class="JobGiver_GetFood"/>
                <li Class="JobGiver_GetRest"/>
                <li Class="JobGiver_GetJoy"/>
                <li Class="JobGiver_Work"/>
              </subNodes>
            </li>
          </subNodes>
        </li>
      </subNodes>
    </thinkRoot>
  </ThinkTreeDef>
  
  <ThinkTreeDef>
    <defName>MAndCoPostMain</defName>
    <insertTag>Humanlike_PostMain</insertTag>
    <insertPriority>100</insertPriority>
    <thinkRoot Class="ThinkNode_Priority">
      <subNodes>
        <li Class="OutpostGenerator.ThinkNode_ConditionalMAndCoEmployee">
          <subNodes>
            <li Class="JobGiver_WanderAnywhere">
              <maxDanger>Deadly</maxDanger>
            </li>
            <li Class="JobGiver_IdleError"/>
          </subNodes>
        </li>
      </subNodes>
    </thinkRoot>
  </ThinkTreeDef>
  
</ThinkTrees>