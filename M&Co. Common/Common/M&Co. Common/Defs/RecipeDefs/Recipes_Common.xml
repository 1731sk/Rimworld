<?xml version="1.0" encoding="utf-8" ?>
<RecipeDefs>

  <RecipeDef>
    <defName>MakeIntegratedCircuitsBoard</defName>
    <label>Make integrated circuits board</label>
    <description>   Using metal and silver, make some home-made integrated circuits boards. It can be used in any electronic device.
Even if you pay the greatest attention making it, it is still fragile and unreliable.</description>
    <jobString>Making integrated circuits board.</jobString>
    <workAmount>800</workAmount>
    <workSpeedStat>ElectronicCraftingSpeed</workSpeedStat>
    <efficiencyStat>ElectronicCraftingEfficiency</efficiencyStat>
    <skillRequirements>
      <li>
        <skill>Crafting</skill>
        <minLevel>3</minLevel>
      </li>
    </skillRequirements>
    <effectWorking>ButcherMechanoid</effectWorking>
    <soundWorking>Recipe_ButcherCorpseMechanoid</soundWorking>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <IntegratedCircuitsBoard>5</IntegratedCircuitsBoard>
    </products>
    <workSkill>Crafting</workSkill>
  </RecipeDef>

</RecipeDefs>